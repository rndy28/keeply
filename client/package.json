{
  "name": "keeply",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen": "graphql-codegen",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier:check": "prettier --config .prettierrc --check .",
    "prettier:format": "prettier --config .prettierrc --write .",
    "cypress:open": "cypress open",
    "cypress:open:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "cypress:run": "cypress run",
    "cypress:run:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "test": "jest --env=jsdom",
    "prepare": "cd .. && husky install client/.husky",
    "postinstall": "yarn prepare"
  },
  "lint-staged": {
    "**/*": "yarn prettier:check"
  },
  "dependencies": {
    "@tabler/icons": "^1.70.0",
    "@urql/exchange-graphcache": "^4.4.3",
    "framer-motion": "^6.3.11",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.9.1",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1",
    "react": "^18.0.0",
    "react-calendar": "^3.7.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.32.1",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "urql": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@graphql-codegen/cli": "^2.8.1",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-urql": "^3.6.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.2.0",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/google-one-tap": "^1.2.0",
    "@types/google.accounts": "^0.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.0.0",
    "@types/react-calendar": "^3.5.1",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^10.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^3.0.0"
  }
}
